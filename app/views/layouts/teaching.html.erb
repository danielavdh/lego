<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en" lang="en">
<head>
  <meta http-equiv="content-type" content="text/html; charset=UTF-8" />
  <meta http-equiv="content-script-type" content="text/javascript" />
  <meta http-equiv="content-style-type" content="text/css" />
  <meta http-equiv="content-language" content="en" />
  <meta name="author" content="http://wwwebsites.com" />
  <meta name="copyright" content="2009, Daniela van der Heijden" />
  <meta name="description" content='cello' />
  <meta name="keywords" content='cello' />
  <meta http-equiv="X-UA-Compatible" content="IE=8" />
  <title>Leonid Gorokhov, teaching</title>
  <%= stylesheet_link_tag 'teaching', 'event_calendar'  %>
  <%= javascript_include_tag :defaults, 'browser_detect', 'swfobject', 'event_calendar' %>
  <script src="http://maps.google.com/maps?file=api&amp;v=2&amp;sensor=false&amp;key=<%= @key %>" type="text/javascript"></script>
</head>
<body onload="leonid.page.resizeText(); leonid.links.show_page('<%=action_name%>', '#a3a3bb'); <%= @focus %>; leonid.maps.initialize('<%=action_name%>', '<%=RAILS_ENV%>', '<%=@map%>');" onunload="GUnload('<%=RAILS_ENV%>')" onresize="leonid.page.resizeText();">
	
	<% if action_name=="index" -%>
	<img id="<%= action_name %>_bkg_img"
		 src="/images/teaching/<%= action_name %>.jpg" />

	<div id="<%= action_name %>_bar">
	</div>
	<% end -%>

	<%= yield -%>

	<%= render :partial => 'shared/lang_log' %>
	<%= render :partial => 'shared/nav_teaching' %>


</body>
</html>

